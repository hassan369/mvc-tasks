@model WebApplication1.Models.Student

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">Major</th>
            <th scope="col">Faculity</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in ViewBag.Student)
        {
            <tr>
                <th scope="row">@item.ID</th>
                <td>@item.Name</td>
                <td>@item.Major</td>
                <td>@item.Faculity</td>
            </tr>
        }
        
       
    </tbody>
</table>